import "../../../../../../org.thingml.samples/src/main/thingml/core/_linux/test.thingml"
import "../testMaskProperty.thingml"
import "../tester.thingml"
import "../../../../../../org.thingml.samples/src/main/thingml/core/_linux/timer.thingml"

configuration TestMaskProperty {
	instance timer : TimerLinux
	instance harness : Tester
	instance dump : TestDumpLinux
	instance test : TestMaskProperty
	connector test.harness => dump.dump
	connector test.harness => harness.test
	connector harness.testEnd => dump.dump
