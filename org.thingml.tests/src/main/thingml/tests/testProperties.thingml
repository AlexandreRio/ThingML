import "thingml.thingml"

thing TestProperties includes Test 
@test " # ABC"
{
    property a : Char = '\'A\''
    
	statechart TestProperties init I {
	 
        property b : Char = '\'B\''
                                                                  
		composite state I init J{
            
            property c : Char = '\'C\''
    
			state J {
				on entry do
					harnessOut!testOut(a)
					harnessOut!testOut(b)
					harnessOut!testOut(c)
				end
            }
		}	
	}
}
