import "thingml.thingml"

thing TestSession includes Test 
@test "AB # AABB"
{
    property id : Char
    
	statechart TestSession init I {
	 
        state I {
			internal event m : harnessIn?testIn
			action do
				id = m.c
        			fork s
			end
		}
		
		session s init Init {						
			state Init {
				on entry do
					harnessOut!testOut(id)
				end
				transition -> Stop
			}
			
			final state Stop {
				on entry do
					harnessOut!testOut(id)
				end
			}
		}
	}
}
