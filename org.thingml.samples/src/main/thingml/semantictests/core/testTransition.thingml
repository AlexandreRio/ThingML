import "testharness.thingml"

thing TestTransition includes Test 
@test "t # I"
@test "tt # IF"
@test "ttt # IFI"
@test "ttsdfgtazerttt # IFIFIF"
{
	statechart TestTransition init I {
	
		state I {
			transition -> F
			event m : harness?testIn
			guard m.c == '\'t\''
			action harness!testOut('\'I\'')
		}
		state F {
			transition -> I
			event m : harness?testIn
			guard m.c == '\'t\''
			action harness!testOut('\'F\'')
		}
	}
}