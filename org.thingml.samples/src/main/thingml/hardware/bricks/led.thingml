import "../../thingml.thingml"

thing fragment LedMsgs
{
	message led_on ();
	message led_off ();
	message led_toggle ();
}

thing fragment Led includes LedMsgs
{   
	provided port Led 
    {
		receives led_on, led_off, led_toggle
	}
}